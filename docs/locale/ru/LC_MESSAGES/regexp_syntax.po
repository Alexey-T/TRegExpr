msgid "``\\W``"
msgstr "``\\W``"

# 3ce4cb92e41748be8fc64bb6265b7f0d
#: ../../regexp_syntax.rst:96
msgid "a nonalphanumeric"
msgstr "не буквенно-цифровой"

