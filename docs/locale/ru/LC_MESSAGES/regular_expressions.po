msgstr "``\\b``"

# b050c0d2a8964146b3995387487284c1
#: ../../regular_expressions.rst:246
msgid "a word boundary"
msgstr "разделитель слов"

# 4d968b3480aa40e6ac324c0a7fcbb3be
#: ../../regular_expressions.rst:247
msgid "``\\B``"
msgstr "``\\B``"

# 6228e0b654fb49c4ad39048de05e0bd2
