msgstr "un caractère numérique (identique à ``[0123456789]``)"

# b05afcd0b23b492c8684a233d000dcaa
#: ../../regular_expressions.rst:149
msgid "``\\D``"
msgstr "`` \\ D``"

# 5e5606c3c057494f82703e06fc80d761
#: ../../regular_expressions.rst:149
msgid "a non-numeric"
msgstr "un non numérique"

# c514858363dc4b8f96f62aa987c2e2e3
#: ../../regular_expressions.rst:150
msgid "``\\s``"
msgstr "``\\s``"

# a5742539a5e2465aa42ebaeeae2eb180
#: ../../regular_expressions.rst:150
msgid "any space (same as ``[ \\t\\n\\r\\f]``)"
msgstr "N&#39;importe quel espace (identique à ``[\\t\\n\\r\\f]``)"

# 868026bfce6543d1b0f00341f427b66c
#: ../../regular_expressions.rst:151
msgid "``\\S``"
msgstr "`` \\ S``"

# ac0d5e5a5a7a4c07a2d8071ffdd5bb70
#: ../../regular_expressions.rst:151
msgid "a non space"
msgstr "un non espace"

# 714b08c429d44c62a9620a407ba3dfb7
#: ../../regular_expressions.rst:154
msgid ""
"You may use ``\\w``, ``\\d`` and ``\\s`` within `user character classes "
"<User Character Classes_>`_."
msgstr ""
"Vous pouvez utiliser ``\\w``, ``\\d`` et ``\\s`` dans `les classes de "
"caractères de l&#39;utilisateur <User Character Classes_>`_."

# 5bf7190aae7a4c449ba725816314ed6f
#: ../../regular_expressions.rst:160
msgid "``foob\\dr``"
msgstr "`` foob \\ dr``"

# 5ecb1b6a1c634723ace15fca6ca25e78
#: ../../regular_expressions.rst:160
msgid "``foob1r``, ``foob6r`` and so on but not ``foobar``, ``foobbr`` and so on"
msgstr ""
"``foob1r``, ``foob6r`` et ainsi de suite mais pas ``foobar``, ``foobbr`` "
"et ainsi de suite"

# 96eec08b9694452db9562c1ab22d1cc2
