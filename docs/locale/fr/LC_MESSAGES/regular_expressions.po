msgid "``foob\\dr``"
msgstr "`` foob \\ dr``"

# 8f736c2a4bc745cbac67694028d71acb
#: ../../regular_expressions.rst:150
msgid ""
"``foob1r``, ``foob6r`` and so on but not ``foobar``, ``foobbr`` and so on"
msgstr ""
"`` foob1r``, `` foob6r`` et ainsi de suite mais pas `` foobar``, `` foobbr``"
" et ainsi de suite"

# dfb845b2a2594c7ab40c1b72f119bc8d
