msgstr "un no alfanumérico"

# 46b7d80f33eb4181bb42e4c36b1c575a
#: ../../regular_expressions.rst:148
msgid "``\\d``"
msgstr "`` \\ d``"

# 7013ab0d756b42489d52c7ebdb9179e0
#: ../../regular_expressions.rst:148
