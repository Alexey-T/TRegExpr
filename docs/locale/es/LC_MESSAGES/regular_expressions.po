msgstr "``aaaa`` y ``cc``"

# 89ada9bdf55647f89603fd92ca6a65ac
#: ../../regular_expressions.rst:411
msgid "``(.+)\\1+``"
msgstr "``(.+)\\1+``"

# a338a3f52277450696dec5d2a66120ed
#: ../../regular_expressions.rst:411
