#: ../../regular_expressions.rst:406
msgid "Backreferences"
msgstr "Referencias"

# 84c84b6c556c41c39c0aa251581dbf96
#: ../../regular_expressions.rst:408
msgid ""
"Metacharacters ``\\1`` through ``\\9`` are interpreted as backreferences."
" ``\\n`` matches previously matched subexpression ``n``."
msgstr ""
"Los metacaracteres `` \\ 1`` a través de `` \\ 9`` se interpretan como "
"referencias inversas. ``\\n`` coincide con la subexpresión previamente "
"coincidente ``n``."

# 89d606adebd74c70830b482fe1afbbc4
#: ../../regular_expressions.rst:414
msgid "``(.)\\1+``"
msgstr "``(.)\\1+``"

# ca575268a0d8414c8af6afa3de4a1c1a
#: ../../regular_expressions.rst:414
msgid "``aaaa`` and ``cc``"
msgstr "``aaaa`` y ``cc``"

# 04e980a6c73b401fa80de30dd6191c69
#: ../../regular_expressions.rst:415
msgid "``(.+)\\1+``"
msgstr "``(.+)\\1+``"

# 77f03494aee84aa995715f14d1dd252d
#: ../../regular_expressions.rst:415
