msgstr "`` foob \\ dr``"

# 5ecb1b6a1c634723ace15fca6ca25e78
#: ../../regular_expressions.rst:160
msgid "``foob1r``, ``foob6r`` and so on but not ``foobar``, ``foobbr`` and so on"
msgstr ""
"``foob1r``, ``foob6r`` y así sucesivamente, pero no ``foobar``, "
"``foobbr`` y así sucesivamente"

# 96eec08b9694452db9562c1ab22d1cc2
#: ../../regular_expressions.rst:161
msgid "``foob[\\w\\s]r``"
msgstr "`` foob [\\ w \\ s] r``"

# d67799ae980e4b548f69c5521651f30a
#: ../../regular_expressions.rst:161
msgid ""
"``foobar``, ``foob r``, ``foobbr`` and so on but not ``foob1r``, "
"``foob=r`` and so on"
msgstr ""
"``foobar``, `` foob r``, ``foobbr`` y así sucesivamente, pero no "
"``foob1r``, `` foob = r`` y así sucesivamente"

